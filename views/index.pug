doctype html
include ./module
html
    head
        link(rel="stylesheet" href="css/index.css" type="text/css")
        script(src="https://kit.fontawesome.com/33f06ad48a.js" crossorigin="anonymous")
        script(src="js/ws.js" type="text/javascript")
    body
        div#yell
        div#fps
        canvas#stage
        div#menu
            div#nick
                input#name.input-default(placeholder="표시될 닉네임" autocomplete="off" style="width: 200px;")
                button#setName.button-default= "변경"
            div#chatting
                div#chatList
                input#chatInput.input-default(placeholder="채팅을 입력하세요." autocomplete="off" style="height:20px; width: 90%")
        div#players
        div#objects
        script(src="/lang" type="text/javascript")
        script(src="js/const.js" type="text/javascript")
        script(src="js/tool/lang.js" type="text/javascript")
        script(src="js/tool/command.js" type="text/javascript")
        script(src="js/tool/map.js" type="text/javascript")
        script(src="js/tool/entity.js" type="text/javascript")
        script(src="js/tool/player.js" type="text/javascript")
        script(src="js/tool/mob.js" type="text/javascript")
        script(src="js/index.js" type="text/javascript")